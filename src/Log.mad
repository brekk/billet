import type { Tag } from "@/Tag"

import IO from "IO"

import { debugged } from "@/Debug"
import P from "@/Processor"



raw :: Show a => List Tag -> Tag -> a -> a
export raw = P.composed(IO.pTrace)

parsed :: Show a => List Tag -> String -> a -> a
export parsed = P.parsed(IO.pTrace)

debug :: String -> Dictionary String String -> String -> String -> String
export debug = debugged(IO.pTrace)
